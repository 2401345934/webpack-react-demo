  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "ahooks": "^3.7.8",
    "antd": "^5.10.1",
    "awesome-only-resolves-last-promise": "^1.0.3",
    "axios": "^1.5.1",
    "crypto-js": "^4.1.1",
