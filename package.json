{
  "name": "webpack-react-test",
  "version": "1.8.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "version": "# 修改 version  \n zx bump.mjs",
    "create": "node template/index.js",
    "commit": "tsc --noEmit && npm run version && npm run changelog && git add . && git cz ",
    "push": "npm run commit && git push origin HEAD",
    "start": "npm run dev",
    "dev": "npx webpack-dev-server --hot --mode=development --progress",
    "qa": "npx webpack-dev-server --hot --progress --mode=development",
    "prod": "npx webpack-dev-server --hot --mode=production --progress",
    "build": "npx webpack --mode=production --progress",
    "build:dev": "npx webpack --mode=production --progress",
    "build:qa": "npx webpack --mode=production --progress ",
    "build:prod": "npx webpack --mode=production --progress",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint ./src/**/*.tsx --fix",
    "lint:style": "stylelint src/**/*.less --fix",
    "changelog": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "parser": "babel-eslint"
  },
  "lint-staged": {
    "src/**/*.ts?(x)": [
      "tsc --noEmit",
      "git add"
    ],
    "src/**/*.js": [
      "prettier --config .prettierrc.js --write",
      "eslint --fix",
      "git add"
    ],
    "src/**/*.less": [
      "prettier --config .prettierrc.js --write",
      "stylelint --fix --allow-empty-input",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@commitlint/config-conventional": "^17.6.3",
    "@statoscope/webpack-plugin": "^5.26.2",
    "@types/lodash": "^4.14.194",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "autoprefixer": "^10.4.14",
    "babel-auto-try-async": "^1.0.0",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.3.0",
    "commitlint": "^17.6.3",
    "compression-webpack-plugin": "^10.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-webpack-plugin": "^4.0.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "happypack": "^5.0.1",
    "html-minimizer-webpack-plugin": "^4.3.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lint-staged": "^13.2.2",
    "mini-css-extract-plugin": "^2.7.5",
    "postcss": "^8.4.23",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^7.3.0",
    "prettier": "^2.8.8",
    "prompts": "^2.4.2",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "terser-webpack-plugin": "^5.3.8",
    "thread-loader": "^4.0.1",
    "typescript": "^5.0.4",
    "typescript-plugin-css-modules": "^5.0.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1",
    "webpack-merge": "^5.8.0",
    "webpack-spritesmith": "^1.1.0",
    "zx": "^7.2.2"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "ahooks": "^3.7.7",
    "antd": "^5.5.0",
    "awesome-only-resolves-last-promise": "^1.0.3",
    "axios": "^1.4.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-react-css-modules": "^5.2.6",
    "commitlint-config-cz": "^0.13.3",
    "crypto-js": "^4.1.1",
    "css-loader": "^6.7.3",
    "cz-customizable": "^7.0.0",
    "dayjs": "^1.11.7",
    "eslint": "^8.40.0",
    "eslint-plugin-react": "^7.32.2",
    "history": "^5.3.0",
    "html-webpack-plugin": "^5.5.1",
    "image-webpack-loader": "^8.1.0",
    "inquirer": "8.2.3",
    "nprogress": "^0.2.0",
    "react-dom": "^18.2.0",
    "react-resizable": "^3.0.5",
    "react-router-dom": "^6.11.1",
    "scroxt": "^1.4.2",
    "style-loader": "^3.3.2",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-less": "^1.0.6",
    "stylelint-order": "^6.0.3",
    "typed.js": "^2.0.132",
    "unplugin-auto-import": "^0.15.3",
    "webpack-dev-server": "^4.15.0",
    "zustand": "^4.3.8"
  }
}
