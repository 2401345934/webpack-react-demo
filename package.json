{
  "name": "webpack-react-test",
  "version": "1.4.7",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "version": "# 修改 version  \n zx bump.mjs",
    "commit": "npm run version && npm run changelog && git add . && git cz ",
    "push": "npm run commit && git push origin HEAD",
    "start": "npx webpack-dev-server --mode=development --progress",
    "dev": "npx webpack-dev-server --mode=development --progress",
    "qa": "npx webpack-dev-server --progress --mode=development",
    "prod": "npx webpack-dev-server --mode=production --progress",
    "build": "npx webpack --mode=production --progress",
    "build:dev": "npx webpack --mode=production --progress",
    "build:qa": "npx webpack --mode=production --progress ",
    "build:prod": "npx webpack --mode=production --progress",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint --ext .jsx ./src --fix",
    "lint:style": "stylelint src/**/*.{scss,wxss} --fix",
    "changelog": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "parser": "babel-eslint"
  },
  "lint-staged": {
    "src/**/*.{js,ts,wxs}": [
      "prettier --config .prettierrc.js --write",
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{wxss,scss}": [
      "prettier --config .prettierrc.js --write",
      "stylelint --fix --allow-empty-input",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@statoscope/webpack-plugin": "^5.25.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.14",
    "babel-auto-try-async": "^1.0.0",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.3.0",
    "commitlint": "^17.4.4",
    "compression-webpack-plugin": "^10.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-webpack-plugin": "^4.0.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "happypack": "^5.0.1",
    "html-minimizer-webpack-plugin": "^4.3.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lint-staged": "^13.2.0",
    "mini-css-extract-plugin": "^2.7.5",
    "postcss": "^8.4.21",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^7.1.0",
    "prettier": "^2.8.4",
    "prompts": "^2.4.2",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "terser-webpack-plugin": "^5.3.7",
    "thread-loader": "^3.0.4",
    "typescript": "^5.0.2",
    "typescript-plugin-css-modules": "^4.2.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.8.0",
    "webpack-spritesmith": "^1.1.0",
    "zx": "^7.2.1"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "ahooks": "^3.7.5",
    "antd": "^5.3.1",
    "awesome-only-resolves-last-promise": "^1.0.3",
    "axios": "^1.3.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-react-css-modules": "^5.2.6",
    "commitlint-config-cz": "^0.13.3",
    "css-loader": "^6.7.3",
    "cz-customizable": "^7.0.0",
    "dayjs": "^1.11.7",
    "eslint": "^8.36.0",
    "eslint-plugin-react": "^7.32.2",
    "history": "^5.3.0",
    "html-webpack-plugin": "^5.5.0",
    "image-webpack-loader": "^8.1.0",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "react-dom": "^18.2.0",
    "react-resizable": "^3.0.4",
    "react-router-dom": "^6.9.0",
    "scroxt": "^1.4.2",
    "style-loader": "^3.3.2",
    "typed.js": "^2.0.132",
    "webpack-dev-server": "^4.13.0"
  }
}
