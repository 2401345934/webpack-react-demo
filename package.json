{
  "name": "webpack-react-test",
  "version": "2.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "version": "# 修改 version  \n zx bump.mjs",
    "create": "node template/index.js",
    "commit": "tsc --noEmit && npm run version && npm run changelog && git add . && git cz ",
    "push": "npm run commit && git push origin HEAD",
    "start": "npm run dev",
    "dev": "npx webpack-dev-server --hot --mode=development --progress",
    "qa": "npx webpack-dev-server --hot --progress --mode=development",
    "prod": "npx webpack-dev-server --hot --mode=production --progress",
    "build": "npx webpack --mode=production --progress",
    "build:dev": "npx webpack --mode=production --progress",
    "build:qa": "npx webpack --mode=production --progress ",
    "build:prod": "npx webpack --mode=production --progress",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint ./src/**/*.tsx --fix",
    "lint:style": "stylelint src/**/*.less --fix",
    "changelog": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "parser": "babel-eslint"
  },
  "lint-staged": {
    "src/**/*.ts?(x)": [
      "tsc --noEmit",
      "git add"
    ],
    "src/**/*.js": [
      "prettier --config .prettierrc.js --write",
      "eslint --fix",
      "git add"
    ],
    "src/**/*.less": [
      "prettier --config .prettierrc.js --write",
      "stylelint --fix --allow-empty-input",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@commitlint/config-conventional": "^17.8.0",
    "@statoscope/webpack-plugin": "^5.28.0",
    "@svgr/webpack": "^8.1.0",
    "@types/lodash": "^4.14.199",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "autoprefixer": "^10.4.16",
    "babel-auto-try-async": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-react-css-modules": "^5.2.6",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.3.0",
    "commitlint": "^17.8.0",
    "commitlint-config-cz": "^0.13.3",
    "compression-webpack-plugin": "^10.0.0",
    "css-loader": "^6.8.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-webpack-plugin": "^4.0.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^9.0.0",
    "happypack": "^5.0.1",
    "html-minimizer-webpack-plugin": "^4.4.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "image-webpack-loader": "^8.1.0",
    "inquirer": "9.2.11",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "lint-staged": "^15.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.31",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.0.3",
    "prompts": "^2.4.2",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.3.3",
    "stylelint": "^15.10.3",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-less": "^2.0.0",
